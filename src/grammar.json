{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "abap",
  "rules": {
    "source": {
      "type": "REPEAT",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "_at_least_one_ws"
            }
          },
          {
            "type": "SYMBOL",
            "name": "_statement"
          }
        ]
      }
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_simple_statement"
        }
      ]
    },
    "_simple_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "inline_declaration"
        },
        {
          "type": "SYMBOL",
          "name": "data_declaration"
        }
      ]
    },
    "_compound_statement": {
      "type": "CHOICE",
      "members": []
    },
    "inline_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "data",
              "flags": "i"
            },
            {
              "type": "PATTERN",
              "value": "final",
              "flags": "i"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "data_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "data",
          "flags": "i"
        },
        {
          "type": "SYMBOL",
          "name": "_at_least_one_ws"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_at_least_one_ws"
        },
        {
          "type": "PATTERN",
          "value": "type",
          "flags": "i"
        },
        {
          "type": "SYMBOL",
          "name": "_at_least_one_ws"
        },
        {
          "type": "FIELD",
          "name": "type",
          "content": {
            "type": "SYMBOL",
            "name": "type_reference"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_at_least_one_ws"
          }
        },
        {
          "type": "STRING",
          "value": "."
        }
      ]
    },
    "type_reference": {
      "type": "PATTERN",
      "value": "[a-zA-Z][a-zA-Z0-9_\\/-]*"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_\\/][a-zA-Z0-9_\\/-]*"
    },
    "field_symbol": {
      "type": "PATTERN",
      "value": "[a-zA-Z][a-zA-Z0-9_\\/-<>]*"
    },
    "_at_least_one_ws": {
      "type": "PATTERN",
      "value": "\\s+"
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [
    "_simple_statement",
    "_compound_statement"
  ],
  "reserved": {}
}
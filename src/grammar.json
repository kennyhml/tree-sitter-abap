{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "abap",
  "rules": {
    "source": {
      "type": "REPEAT",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "REPEAT",
            "content": {
              "type": "PATTERN",
              "value": "\\s+"
            }
          },
          {
            "type": "SYMBOL",
            "name": "_statement"
          }
        ]
      }
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_simple_statement"
        }
      ]
    },
    "_simple_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "inline_declaration"
        },
        {
          "type": "SYMBOL",
          "name": "data_declaration"
        }
      ]
    },
    "_compound_statement": {
      "type": "CHOICE",
      "members": []
    },
    "inline_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "data",
              "flags": "i"
            },
            {
              "type": "PATTERN",
              "value": "final",
              "flags": "i"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "data_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "data",
          "flags": "i"
        },
        {
          "type": "SYMBOL",
          "name": "_at_least_one_ws"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_at_least_one_ws"
        },
        {
          "type": "PATTERN",
          "value": "type",
          "flags": "i"
        },
        {
          "type": "SYMBOL",
          "name": "_at_least_one_ws"
        },
        {
          "type": "FIELD",
          "name": "type",
          "content": {
            "type": "SYMBOL",
            "name": "type_reference"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "FIELD",
                      "name": "length",
                      "content": {
                        "type": "SYMBOL",
                        "name": "_data_length"
                      }
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "FIELD",
                      "name": "value",
                      "content": {
                        "type": "SYMBOL",
                        "name": "_data_value"
                      }
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                }
              ]
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "FIELD",
                      "name": "value",
                      "content": {
                        "type": "SYMBOL",
                        "name": "_data_value"
                      }
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "FIELD",
                      "name": "length",
                      "content": {
                        "type": "SYMBOL",
                        "name": "_data_length"
                      }
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "PATTERN",
            "value": "\\s+"
          }
        },
        {
          "type": "STRING",
          "value": "."
        }
      ]
    },
    "_data_value": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_at_least_one_ws"
        },
        {
          "type": "PATTERN",
          "value": "value",
          "flags": "i"
        },
        {
          "type": "SYMBOL",
          "name": "_at_least_one_ws"
        },
        {
          "type": "SYMBOL",
          "name": "literal_string"
        }
      ]
    },
    "_data_length": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_at_least_one_ws"
        },
        {
          "type": "PATTERN",
          "value": "length",
          "flags": "i"
        },
        {
          "type": "SYMBOL",
          "name": "_at_least_one_ws"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "literal_int"
            },
            {
              "type": "SYMBOL",
              "name": "literal_string"
            }
          ]
        }
      ]
    },
    "type_reference": {
      "type": "PATTERN",
      "value": "[a-zA-Z][a-zA-Z0-9_\\/-]*"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_\\/][a-zA-Z0-9_\\/-]*"
    },
    "field_symbol": {
      "type": "PATTERN",
      "value": "[a-zA-Z][a-zA-Z0-9_\\/-<>]*"
    },
    "literal_string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "'"
            },
            {
              "type": "FIELD",
              "name": "content",
              "content": {
                "type": "PATTERN",
                "value": "[^']*"
              }
            },
            {
              "type": "STRING",
              "value": "'"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "`"
            },
            {
              "type": "FIELD",
              "name": "content",
              "content": {
                "type": "PATTERN",
                "value": "[^`]*"
              }
            },
            {
              "type": "STRING",
              "value": "`"
            }
          ]
        }
      ]
    },
    "literal_int": {
      "type": "PATTERN",
      "value": "-?\\d+"
    },
    "_at_least_one_ws": {
      "type": "PATTERN",
      "value": "\\s+"
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [
    "_simple_statement",
    "_compound_statement"
  ],
  "reserved": {}
}
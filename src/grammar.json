{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "abap",
  "rules": {
    "source": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_statement"
      }
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_simple_statement"
        }
      ]
    },
    "_simple_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "inline_declaration"
        },
        {
          "type": "SYMBOL",
          "name": "data_declaration"
        },
        {
          "type": "SYMBOL",
          "name": "inline_decl_assignment"
        }
      ]
    },
    "_compound_statement": {
      "type": "CHOICE",
      "members": []
    },
    "inline_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "ALIAS",
              "content": {
                "type": "PATTERN",
                "value": "[dD][aA][tT][aA]"
              },
              "named": false,
              "value": "data"
            },
            {
              "type": "ALIAS",
              "content": {
                "type": "PATTERN",
                "value": "[fF][iI][nN][aA][lL]"
              },
              "named": false,
              "value": "final"
            }
          ]
        },
        {
          "type": "IMMEDIATE_TOKEN",
          "content": {
            "type": "STRING",
            "value": "("
          }
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "IMMEDIATE_TOKEN",
          "content": {
            "type": "STRING",
            "value": ")"
          }
        }
      ]
    },
    "inline_decl_assignment": {
      "type": "PREC",
      "value": 10,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "ALIAS",
                "content": {
                  "type": "PATTERN",
                  "value": "[dD][aA][tT][aA]"
                },
                "named": false,
                "value": "data"
              },
              {
                "type": "ALIAS",
                "content": {
                  "type": "PATTERN",
                  "value": "[fF][iI][nN][aA][lL]"
                },
                "named": false,
                "value": "final"
              }
            ]
          },
          {
            "type": "IMMEDIATE_TOKEN",
            "content": {
              "type": "STRING",
              "value": "("
            }
          },
          {
            "type": "FIELD",
            "name": "name",
            "content": {
              "type": "SYMBOL",
              "name": "identifier"
            }
          },
          {
            "type": "IMMEDIATE_TOKEN",
            "content": {
              "type": "STRING",
              "value": ")"
            }
          },
          {
            "type": "STRING",
            "value": "="
          },
          {
            "type": "FIELD",
            "name": "value",
            "content": {
              "type": "SYMBOL",
              "name": "_expression"
            }
          },
          {
            "type": "STRING",
            "value": "."
          }
        ]
      }
    },
    "_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "literal_int"
        },
        {
          "type": "SYMBOL",
          "name": "literal_string"
        }
      ]
    },
    "data_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[dD][aA][tT][aA]"
          },
          "named": false,
          "value": "data"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "bufsize",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": "("
                  },
                  {
                    "type": "SYMBOL",
                    "name": "literal_int"
                  },
                  {
                    "type": "STRING",
                    "value": ")"
                  }
                ]
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "FIELD",
                    "name": "type",
                    "content": {
                      "type": "SYMBOL",
                      "name": "_type_reference"
                    }
                  },
                  {
                    "type": "FIELD",
                    "name": "like",
                    "content": {
                      "type": "SYMBOL",
                      "name": "_like_reference"
                    }
                  },
                  {
                    "type": "FIELD",
                    "name": "length",
                    "content": {
                      "type": "SYMBOL",
                      "name": "_data_length"
                    }
                  },
                  {
                    "type": "FIELD",
                    "name": "value",
                    "content": {
                      "type": "SYMBOL",
                      "name": "_data_value"
                    }
                  },
                  {
                    "type": "FIELD",
                    "name": "decimals",
                    "content": {
                      "type": "SYMBOL",
                      "name": "_data_decimals"
                    }
                  },
                  {
                    "type": "FIELD",
                    "name": "readonly",
                    "content": {
                      "type": "SEQ",
                      "members": [
                        {
                          "type": "ALIAS",
                          "content": {
                            "type": "PATTERN",
                            "value": "[rR][eE][aA][dD]-[oO][nN][lL][yY]"
                          },
                          "named": false,
                          "value": "read-only"
                        }
                      ]
                    }
                  }
                ]
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "."
        }
      ]
    },
    "_type_reference": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[tT][yY][pP][eE]"
          },
          "named": false,
          "value": "type"
        },
        {
          "type": "SYMBOL",
          "name": "type"
        }
      ]
    },
    "_like_reference": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[lL][iI][kK][eE]"
          },
          "named": false,
          "value": "like"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "_data_value": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[vV][aA][lL][uU][eE]"
          },
          "named": false,
          "value": "value"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "literal_string"
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "ALIAS",
                  "content": {
                    "type": "PATTERN",
                    "value": "[iI][sS]"
                  },
                  "named": false,
                  "value": "is"
                },
                {
                  "type": "ALIAS",
                  "content": {
                    "type": "PATTERN",
                    "value": "[iI][nN][iI][tT][iI][aA][lL]"
                  },
                  "named": false,
                  "value": "initial"
                }
              ]
            }
          ]
        }
      ]
    },
    "_data_length": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[lL][eE][nN][gG][tT][hH]"
          },
          "named": false,
          "value": "length"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "literal_int"
            },
            {
              "type": "SYMBOL",
              "name": "literal_string"
            }
          ]
        }
      ]
    },
    "_data_decimals": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[dD][eE][cC][iI][mM][aA][lL][sS]"
          },
          "named": false,
          "value": "decimals"
        },
        {
          "type": "SYMBOL",
          "name": "literal_int"
        }
      ]
    },
    "type": {
      "type": "PATTERN",
      "value": "[a-zA-Z\\/][a-zA-Z0-9_\\/-]*"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_\\/][a-zA-Z0-9_\\/-]*"
    },
    "field_symbol": {
      "type": "PATTERN",
      "value": "[a-zA-Z][a-zA-Z0-9_\\/-<>]*"
    },
    "literal_string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "'"
            },
            {
              "type": "FIELD",
              "name": "content",
              "content": {
                "type": "PATTERN",
                "value": "[^']*"
              }
            },
            {
              "type": "STRING",
              "value": "'"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "`"
            },
            {
              "type": "FIELD",
              "name": "content",
              "content": {
                "type": "PATTERN",
                "value": "[^`]*"
              }
            },
            {
              "type": "STRING",
              "value": "`"
            }
          ]
        }
      ]
    },
    "literal_int": {
      "type": "PATTERN",
      "value": "-?\\d+"
    },
    "_at_least_one_ws": {
      "type": "PATTERN",
      "value": "\\s+"
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "*"
              },
              {
                "type": "STRING",
                "value": "\""
              }
            ]
          },
          {
            "type": "PATTERN",
            "value": "[^\\n\\r]*"
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "_whitespace"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "_whitespace"
    },
    {
      "type": "SYMBOL",
      "name": "_error_sentinel"
    }
  ],
  "inline": [],
  "supertypes": [
    "_simple_statement",
    "_compound_statement"
  ],
  "reserved": {}
}